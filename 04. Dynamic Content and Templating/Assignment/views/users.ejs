<%- include('includes/head.ejs') %>
    <link rel="stylesheet" href="css/users.css">
</head>
<body>
<%- include('includes/header.ejs') %>
    <main>
        <% if (users.length > 0) { %>
            <ul class="student-list">
                <h1>Users</h1>
                <% for (user of users) { %>
                    <li class="student-list__item"><%= user.firstName + ' ' + user.surname %></li>
                <% } %>
            </ul>
        <% } else { %>
            <h1>No users</h1>
        <% } %>
    </main>
</body>
</html>